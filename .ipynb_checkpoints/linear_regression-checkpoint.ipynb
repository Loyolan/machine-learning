   "source": [
    "import matplotlib.pyplot as plt\n",
    "from sklearn.datasets import load_iris\n",
    "\n",
    "fig, ax = plt.subplots(1, 1, sharex=True)\n",
    "iris = load_iris()\n",
    "x = iris.data\n",
    "y = iris.target\n",
    "ax = plt.axes(projection='3d')\n",
    "ax.scatter(x[:, 0], x[:, 1], x[:, 2], c=y)\n",
    "plt.show()"
   ]
